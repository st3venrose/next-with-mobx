{
    "env": {
        "browser": true,
        "es6": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": ["src"]
        }
      }
    },
    "globals": { },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
          "impliedStrict": true
        }
      },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-no-target-blank": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "no-restricted-imports": [2, { "patterns": ["../src"] }]
    }
  }